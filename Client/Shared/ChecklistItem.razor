<MudCheckBox @bind-Value:get="Values[Key]" @bind-Value:set="(value) => SetFunc(Key, value)" T="bool">
    @ChildContent
</MudCheckBox>

@code {
    [Parameter]
    public required Action<string, bool> SetFunc { get; set; }
    
    [Parameter]
    public required string Key { get; set; }
    
    [Parameter]
    public required Dictionary<string, bool> Values { get; set; }
    
    [Parameter]
    public RenderFragment? ChildContent { get; set; }
}